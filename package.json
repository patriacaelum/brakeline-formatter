{
  "name": "brakeline-formatter",
  "version": "1.0.0",
  "description": "An autoformatter that imposes a character limit for each line of text.",
  "main": "src/main.js",
  "scripts": {
    "build": "tsc -noEmit -skipLibCheck --project tsconfig.prod.json && node esbuild.config.mjs production",
    "dev": "tsc -noEmit -skipLibCheck --project tsconfig.dev.json && node esbuild.config.mjs",
    "lint": "eslint src/",
    "test": "jest",
    "version": "node version-bump.mjs && git add manifest.json versions.json"
  },
  "keywords": [],
  "author": "patriacaelum",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1"
  },
  "dependencies": {
    "@types/node": "^16.18.58",
    "builtin-modules": "3.3.0",
    "esbuild": "0.17.3",
    "obsidian": "^1.4.11",
    "ts-node": "^10.9.1",
    "tslib": "2.4.0",
    "typescript": "^4.7.4"
  }
}
